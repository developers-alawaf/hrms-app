    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/login"
          }
        },
        {
          "name": "Accept Invitation",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"invitation-token\",\n  \"password\": \"newPassword\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/accept-invitation"
          }
        },
        {
          "name": "Change Password",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" },
              { "key": "Authorization", "value": "Bearer {{token}}" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"oldPassword\": \"currentPassword\",\n  \"newPassword\": \"newPassword\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/change-password"
          }
        },
        {
          "name": "Request Password Reset",
          "request": {
            "method": "POST",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": "{{baseUrl}}/api/auth/reset-password/:userId"
          }
        },
        {
          "name": "Reset Password (with token)",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"reset-token\",\n  \"newPassword\": \"newPassword\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/reset-password"
          }
        },
        {
          "name": "Resend Invitation",
          "request": {
            "method": "POST",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": "{{baseUrl}}/api/auth/resend-invitation"
